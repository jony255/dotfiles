# don't need usage of all 9 workspaces
bindsym {
    $mod+1 workspace 1
    $mod+2 workspace 2
    $mod+3 workspace 3
    $mod+4 workspace 4
}

bindsym {
    $mod+1 move container to workspace 1
    $mod+2 move container to workspace 2
    $mod+3 move container to workspace 3
    $mod+4 move container to workspace 4
}
